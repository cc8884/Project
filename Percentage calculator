include<stdio.h>
#include<conio.h>
int main()
{
    int i;
    float mark, sum=0, avg,percentage,Physics,Chemistry,Maths,CS,English;
    printf("\nEnter Marks obtained in Physics: ");
    scanf("%f",& Physics);
    printf("\nEnter Marks obtained in Chemistry:");
    scanf("%f",& Chemistry);
    printf("\nEnter Marks obtained in Maths:");
    scanf("%f",& Maths);
    printf("\nEnter Marks obtained in CS:");
    scanf("%f",& CS);
    printf("\nEnter Marks obtained in English:");
    scanf("%f",& English);
    sum=Physics+Chemistry+Maths+CS+English;
    avg=sum/5;
    percentage=sum/500*100;
    printf("\nAverage Mark = %0.2f", avg);
    return 0;
}
